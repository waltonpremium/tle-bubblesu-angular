 <script type="text/ng-template" id="funkyPhonicsModal.html">
      <div class="modal-container">
         <div class="modal-header">
            <div class="row">
                <div class="col-xs-9">
                    <h3 class="modal-title">{{homeModalVm.object.SiteObject.name}}</h3>
                </div>
                <div class="col-xs-3">
                    <img src="//cdn.bubblesu.com/images/buttons/close-button.png" class="modal-close video-close img-responsive center-block" data-ng-click="homeModalVm.cancel()" role="button" alt="Close Funky Phonics">
                </div>
            </div>
         </div>
         <div class="modal-body">
             <div>
                 <img class="funky-phonics img-responsive center-block" data-ng-src="{{homeModalVm.object.SiteObject.path}}" alt="{{homeModalVm.object.SiteObject.name}}">
             </div>
         </div>
         <div class="modal-footer">
             <div class="row">
                 <div class="col-xs-12">
                     <img src="//cdn.bubblesu.com/images/buttons/print-button.png" data-ng-click="homeModalVm.printImage()" class="img-responsive center-block" role="button" alt="Print Current Page">
                 </div>
             </div>
         </div>
     </div>
 </script>

 <script type="text/ng-template" id="recipesModal.html">
      <div class="modal-container">
         <div class="modal-header">
            <div class="row">
                <div class="col-xs-9">
                    <h3 class="modal-title">{{homeModalVm.object.SiteObject.name}}</h3>
                </div>
                <div class="col-xs-3">
                    <img src="//cdn.bubblesu.com/images/buttons/close-button.png" class="modal-close video-close img-responsive center-block" data-ng-click="homeModalVm.cancel()" role="button" alt="Close Funky Phonics">
                </div>
            </div>
         </div>
         <div class="modal-body">
            <div class="row">
                <div class="col-xs-12 col-sm-3">
                    <div>
                        <img data-ng-src="{{homeModalVm.object.SiteObject.thumbnail_path}}" alt="{{homeModalVm.object}}" class="img-responsive center-block">
                    </div>
                </div>
                <div class="col-xs-12 col-sm-9 text-center">
                    <p>Clicking the button below will open the recipe in a new window.</p>
                    <a href="{{homeModalVm.object.SiteObject.path}}" class="btn btn-recipes" target="_blank" role="button">View {{homeModalVm.object.SiteObject.name}}</a>
                </div>
            </div>
         </div>
         <div class="modal-footer">
             <button class="btn btn-warning" type="button" data-ng-click="homeModalVm.cancel()">Close</button>
         </div>
     </div>
 </script>

 <script type="text/ng-template" id="readingCornerModal.html">
     <div style="background-color:orange;">
         <div class="modal-header">
             <h3 class="modal-title">{{homeModalVm.object.SiteObject.name}}</h3>
         </div>
         <div class="modal-body" style="text-align:center;">
             <div>
                 <img data-ng-src="{{homeModalVm.object.SiteObject.thumbnail_path}}" alt="{{homeModalVm.object.SiteObject.name}}">
             </div>
             <div>
                 <audio controls autoplay>
                     <source data-ng-src="{{homeModalVm.object.SiteObject.path}}" type="audio/mpeg" />
                     <a href="{{homeModalVm.object.SiteObject.path}}">{{homeModalVm.object.SiteObject.name}}</a>
                 </audio>
             </div>

         </div>
         <div class="modal-footer">
             <button class="btn btn-warning" type="button" data-ng-click="homeModalVm.cancel()">Close</button>
         </div>
     </div>
    </script>

    <script type="text/ng-template" id="gameModal.html">
         <div class="modal-body" style="text-align:center;">
             <iframe class="game-frame" frameborder="0" data-ng-src="{{homeModalVm.object.SiteObject.path}}" game-frame></iframe>
         </div>
         <div class="modal-footer">
             <button class="btn btn-warning" type="button" data-ng-click="homeModalVm.cancel()">Close</button>
         </div>
    </script>

    <script type="text/ng-template" id="colorCreateModal.html">
        <div class="modal-container">
         <div class="modal-header">
             <div class="row">
                 <div class="col-xs-9">
                     <img data-ng-src="{{homeModalVm.object.headerImage}}" alt="{{homeModalVm.object.headerImageName}}" class="img-responsive">
                 </div>
                 <div class="col-xs-3">
                     <img src="//cdn.bubblesu.com/images/buttons/close-button.png" class="modal-close video-close img-responsive center-block" data-ng-click="homeModalVm.cancel()" role="button" alt="Close Color and Create">
                 </div>
             </div>
         </div>
         <div class="modal-body">
             <div class="owl-carousel owl-theme" id="colorModalSlider">
                 <div data-ng-repeat="item in homeModalVm.object.items" init-color-sliders>
                     <div class="slider-panel">
                         <img data-src="{{item.SiteObject.path}}" class="slider-thumbnail lazyOwl" data-print-src="{{item.SiteObject.path}}" alt="{{item.SiteObject.name}}"  id="{{item.SiteObject.short_name}}">
                     </div>
                 </div>
             </div>
         </div>
         <div class="modal-footer">
             <div class="row">
                 <div class="col-xs-12">
                     <img src="//cdn.bubblesu.com/images/buttons/print-button.png" data-ng-click="homeModalVm.printActiveSlide(homeModalVm.object)" class="img-responsive center-block" role="button" alt="Print Current Page">
                 </div>
             </div>
         </div>
        </div>
    </script>
    <script type="text/ng-template" id="hearts-template.html">
        <img src="//cdn.bubblesu.com/images/buttons/heart-inverted.png" data-ng-class="{'thumbnail-love-button': true, 'heart-waiting': heart.isWaiting === true}" data-ng-show="heart.isHearted() === true" data-ng-click="heart.isWaiting === false && heart.unheart()" data-ng-if="userId !== null" alt="Un-heart">
        <img src="//cdn.bubblesu.com/images/buttons/heart-button.png" data-ng-class="{'thumbnail-love-button': true, 'heart-waiting': heart.isWaiting === true}" data-ng-click="heart.isWaiting === false && heart.heart()" data-ng-show="heart.isHearted() === false" data-ng-if="userId !== null" alt="Heart">
    </script>
</body>
</html>
