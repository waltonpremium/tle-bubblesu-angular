             <img src="//cdn.bubblesu.com/images/video_player/top-right-corner.png" class="top-right-corner" />
             <img src="//cdn.bubblesu.com/images/video_player/bottom-left-corner.png" class="bottom-left-corner" />
             <img src="//cdn.bubblesu.com/images/video_player/bottom-right-corner.png" class="bottom-right-corner" />
             <img src="//cdn.bubblesu.com/images/buttons/play-button-big.png" class="video-big-play hidden-mobile img-responsive" onclick="videoPlay($('#video'))" alt="Play {{homeModalVm.object.SiteObject.name}}">
             <div class="progress">
                 <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">
                    <img src="//cdn.bubblesu.com/images/video_player/progress-bar-star.png" class="seek-star">
