    <script type="text/ng-template" id="messagesModal.html">
        <div class="modal-container">
            <div class="modal-header" data-ng-if="messagesVm.object.name !== undefined">
              <div class="media">
                  <div class="media-left">
                      <img data-ng-src="{{messagesVm.object.avatar}}" class="media-object" alt="{{messagesVm.object.name}}">
                  </div>
                  <div class="media-body media-middle">
                      <h3 class="modal-title">Messages from <span class="messages-{{messagesVm.object.shortname}}">{{messagesVm.object.name}}</h3>
                  </div>
              </div>
            </div>
            <div class="modal-body" style="text-align:center;">
              <div class="panel-group" id="message-accordion" role="tablist" aria-multiselectable="false" data-ng-if="messagesVm.object.messages.length>0">
                  <div class="panel panel-default" data-ng-repeat="message in messagesVm.object.messages">
                      <div class="panel-heading" role="tab" id="heading-{{message.CharacterMessage.id}}" data-toggle="collapse" data-parent="#message-accordion" aria-expanded="false" aria-controls="collapse-{{message.CharacterMessage.id}}" data-target="#collapse-{{message.CharacterMessage.id}}" data-ng-click="message.UserMessage.viewed==false && messagesVm.showMessage(message)">
                          <div class="panel-title">
                              <h4 class="message-subject">
                                  {{message.CharacterMessage.subject}}
                                  <span data-ng-if="message.UserMessage.viewed==false" class="badge">New</span>
                                  <span data-ng-if="message.UserMessage.viewed==true" class="badge">Read</span>
                              </h4>
                              <p class="message-date">
                                  <em>{{message.CharacterMessage.created | parseDate | date: 'fullDate'}}</em>
                              </p>
                          </div>
                      </div>
                      <div id="collapse-{{message.CharacterMessage.id}}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="message-{{message.CharacterMessage.id}}">
                          <div class="panel-body">
                              {{message.CharacterMessage.message}}
                          </div>
                      </div>
                  </div>
              </div>
              <p ng-if="messagesVm.object.messages.length==0 || messagesVm.object.messages==undefined"><em>No Messages</em></p>
            </div>
            <div class="modal-footer">
              <button class="btn btn-warning" type="button" ng-click="messagesVm.cancel()">Close</button>
            </div>
        </div>
    </script>
    <script type="text/ng-template" id="videoModal.html">
