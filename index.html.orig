    <script type="text/ng-template" id="postLoginModal.html">
        <div class="modal-container">
            <div class="modal-header">
                <h3 class="modal-title">Welcome to BubblesU</h3>
            </div>
            <div class="modal-body">
                <div>
                    <p>You&#39;re almost ready! Fill in the following form to begin exploring BubblesU!</p>
                </div>
                <form name="postLoginVm.postForm" data-ng-submit="postLoginVm.postForm.$valid && postLoginVm.modifyAccount($event)">
                    <div class="row name-group ng-hide" data-ng-show="postLoginVm.showNameFields">
                        <div class="form-group col-xs-12 col-sm-6" data-ng-class="{'has-error': ( postLoginVm.postForm.firstname.$dirty && postLoginVm.postForm.firstname.$invalid ) || postLoginVm.nameError }">
                              <label for="post-firstname" class="control-label">First Name (<em>Required</em>)</label>
                              <input id="post-firstname" name="firstname" type="text" class="form-control" placeholder="First Name" data-ng-model="postLoginVm.firstname" data-ng-required="postLoginVm.showNameFields" data-ng-disabled="postLoginVm.isSubmitting">
                              <span class="help-block ng-hide" data-ng-show="postLoginVm.postForm.firstname.$dirty && postLoginVm.postForm.firstname.$invalid">
                                Please enter your first name.
                              </span>
                              <span class="help-block ng-hide" data-ng-show="postLoginVm.nameError">
                                {{postLoginVm.nameMessage}}
                              </span>
                        </div>
                        <div class="form-group col-xs-12 col-sm-6" data-ng-class="{'has-error': ( postLoginVm.postForm.lastname.$dirty && postLoginVm.postForm.lastname.$invalid ) || postLoginVm.nameError }">
                              <label for="post-lastname" class="control-label">Last Name (<em>Required</em>)</label>
                              <input id="post-lastname" name="lastname" type="text" class="form-control" placeholder="Last Name" data-ng-model="postLoginVm.lastname" data-ng-required="postLoginVm.showNameFields" data-ng-disabled="postLoginVm.isSubmitting">
                              <span class="help-block ng-hide" data-ng-show="postLoginVm.postForm.lastname.$dirty && postLoginVm.postForm.lastname.$invalid">
                                Please enter your last name.
                              </span>
                              <span class="help-block ng-hide" data-ng-show="postLoginVm.nameError">
                                {{postLoginVm.nameMessage}}
                              </span>
                        </div>
                    </div>
                    <div class="row ng-hide" data-ng-show="postLoginVm.showPasswordFields">
                        <div class="form-group col-xs-12 col-sm-6" data-ng-class="{'has-error': (postLoginVm.postForm.password.$dirty && postLoginVm.postForm.password.$invalid) || postLoginVm.passwordError}">
                            <label for="post-password" class="control-label">New Password (<em>Required</em>)</label>
                            <input id="post-password" name="password" type="password" class="form-control" placeholder="Password" data-ng-model="postLoginVm.password" data-ng-required="postLoginVM.showPasswordFields" data-ng-minlength="6" data-ng-max-length="100" data-ng-disabled="postLoginVm.isSubmitting">
                            <span class="help-block ng-hide" data-ng-show="postLoginVm.postForm.password.$dirty && postLoginVm.postForm.password.$invalid">
                              Please enter a valid password (minimum 6 characters).
                            </span>
                            <span class="help-block ng-hide" data-ng-show="postLoginVm.passwordError === true">
                              {{postLoginVM.passwordMessage}}
                            </span>
                        </div>
                        <div class="form-group col-xs-12 col-sm-6" data-ng-class="{'has-error': postLoginVm.postForm.passwordVerify.$dirty && postLoginVm.postForm.passwordVerify.$invalid}">
                            <label for="post-password-verify" class="control-label">Re-enter Password (<em>Required</em>)</label>
                            <input id="post-password-verify" name="passwordVerify" type="password" class="form-control" placeholder="Password" data-ng-model="postLoginVm.passwordVerify" data-ng-required="postLoginVM.showPasswordFields" data-ng-minlength="6" data-ng-max-length="100" data-ng-disabled="postLoginVm.isSubmitting">
                            <span class="help-block ng-hide" data-ng-show="postLoginVm.postForm.passwordVerify.$dirty && postLoginVm.postForm.passwordVerify.$invalid">
                              Passwords must match.
                            </span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="post-referral" class="control-label">How did you hear about us? (<em>Required</em>)</label>
                        <select id="post-referral" name="referral" class="form-control" ng-model="postLoginVm.howDidYouHear" data-ng-required="true" data-ng-disabled="postLoginVm.isSubmitting === true">
                            <option value="">Choose</option>
                            <option value="1">I am enrolled at The Learning Experience&reg;</option>
                            <option value="34">Referred by a family of The Learning Experience&reg;</option>
                            <option value="40">Employee of The Learning Experience&reg;</option>
                            <option value="45">I have a Promo Code</option>
                            <option value="21">Social Media</option>
                            <option value="42">Online Ad</option>
                            <option value="6">News Article</option>
                            <option value="19">Search Engine</option>
                            <option value="27">Other</option>

                        </select>
                        <div data-ng-class="{'has-error': postLoginVm.postForm.otherreferral.$dirty && postLoginVm.postForm.otherreferral.$invalid }">
                            <input class="form-control ng-hide" data-ng-show="postLoginVm.howDidYouHear==='27'" name="otherreferral" id="post-other-referral" type="text" placeholder="How did you hear about us?" data-ng-model="postLoginVm.otherReferral" data-ng-trim="true" data-ng-required="postLoginVm.howDidYouHear === '27'" data-ng-disabled="postLoginVm.howDidYouHear !== '27' || postLoginVm.isSubmitting === true">
                            <span class="help-block ng-hide" data-ng-show="postLoginVm.postForm.otherreferral.$dirty && postLoginVm.postForm.otherreferral.$invalid">
                                Please let us know how you heard about us.
                            </span>
                        </div>
                    </div>
                    <div class="form-group ng-hide" data-ng-class="{'has-error': postLoginVm.postForm.zipcode.$dirty && postLoginVm.postForm.zipcode.$invalid}" data-ng-show="postLoginVm.howDidYouHear === '1' || postLoginVm.howDidYouHear === '34' || postLoginVm.howDidYouHear === '40'">
                        <label for="post-zipcode" class="control-label">Zip Code (<em>Required</em>)</label>
                        <input id="post-zipcode" name="zipcode" type="text" class="form-control" placeholder="Zip Code" data-ng-model="postLoginVm.zipCode" data-ng-minlength="5" data-ng-maxlength="5" minlength="5" maxlength="5" data-ng-pattern="/[0-9]{5}/" data-ng-required="postLoginVm.howDidYouHear === '1' || postLoginVm.howDidYouHear === '34' || postLoginVm.howDidYouHear === '40'" data-ng-disabled="postLoginVm.isSubmitting === true" data-ng-model-options="{debounce: 1000}">
                        <span class="help-block ng-hide" data-ng-show="postLoginVm.postForm.zipcode.$dirty && postLoginVm.postForm.zipcode.$invalid">
                          Please enter your 5-digit zipcode.
                        </span>
                    </div>
                    <div class="form-group ng-hide" data-ng-show="postLoginVm.showLocations() && postLoginVm.postForm.zipcode.$valid">
                        <label for="post-center">Choose Your Location (<em>Required</em>)</label>
                        <select id="post-center" class="form-control" data-ng-model="postLoginVm.center" data-ng-options="location as location.Center.NAME for location in postLoginVm.locations track by location.Center.ROWID" data-ng-disabled="postLoginVm.isSubmitting === true">
                            <option value="">Choose</option>
                        </select>
                    </div>
                    <div class="form-group" data-ng-class="{'has-error': postLoginVm.postForm.promocode.$dirty && postLoginVm.postForm.promocode.$invalid}">
                        <label for="post-promocode" class="control-label">Promo Code</label>
                        <input id="post-promocode" type="text" name="promocode" class="form-control" placeeholder="Promo Code" data-ng-model="postLoginVm.promocode" data-ng-disabled="postLoginVm.isSubmitting || postLoginVm.howDidYouHear !== '45'" data-ng-required="postLoginVm.howDidYouHear === '45'">
                        <span class="help-block ng-hide" data-ng-show="postLoginVm.postForm.promocode.$dirty && postLoginVm.postForm.promocode.$invalid">
                            Please enter a promo code
                        </span>
                    </div>
                    <button class="ng-hide" type="submit" data-ng-hide="true">Submit</button>
                </form>
                <div class="text-center" data-ng-class="{'alert alert-danger': postLoginVm.modifyError === true}">{{postLoginVm.message}}</div>
                <div class="ng-hide" data-ng-show="postLoginVm.showLoadingStars===true"><img src="//cdn.bubblesu.com/images/Loading-Stars-V2.gif" alt="Loading...Please Wait." class="img-responsive center-block"></div>
            </div>
            <div class="modal-footer">
                <button class="btn btn-warning" type="button" data-ng-disabled="postLoginVm.isSubmitting === true || postLoginVm.postForm.$invalid" data-ng-click="postLoginVm.postForm.$valid && postLoginVm.modifyAccount($event)">Submit</button>
            </div>
        </div>
    </script>
