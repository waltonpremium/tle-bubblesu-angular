    <script type="text/ng-template" id="loginModal.html">
        <div class="modal-container">
            <div class="modal-header">
                <h3 class="modal-title">Login</h3>
            </div>
            <div class="modal-body arial">
                <form name="loginVm.loginForm" data-ng-submit="loginVm.loginForm.$valid && loginVm.login($event)" novalidate>
                    <div class="form-group">
                        <label class="control-label">E-mail Address / Username</label>
                        <input type="text" class="form-control" placeholder="E-mail Address" ng-model="loginVm.username" required autofocus>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Password</label>
                        <input type="password" class="form-control" placeholder="Password" ng-model="loginVm.password" required>
                    </div>
                    <button class="ng-hide" type="submit" data-ng-hide="true" data-ng-cloak>Login</button>
                </form>
                <a data-ng-click="loginVm.revealForgotForm()">Forgot your password?</a>
                <div class="ng-hide" data-ng-show="loginVm.showForgotForm === true" data-ng-cloak>
                    <form name="loginVm.forgotForm" novalidate>
                        <h2>Forgot Your Password?</h2>
                        <p>Enter your e-mail address below and click on "Reset My Password" to recieve an e-mail containing instructions on how to reset your password.</p>
                        <div data-ng-class="{'form-group': true, 'has-error': loginVm.forgotForm.forgotEmail.$dirty && loginVm.forgotForm.forgotEmail.$invalid}">
                            <label for="forgot-email" class="control-label">E-mail Address</label>
                            <input id="forgot-email" type="email" class="form-control" placeholder="E-mail Address" data-ng-model="loginVm.forgotEmail" name="forgotEmail">
                            <span class="help-block ng-hide" data-ng-show="loginVm.forgotForm.forgotEmail.$dirty && loginVm.forgotForm.forgotEmail.$invalid">
                                Please enter a valid e-mail address.
                            </span>
                        </div>
                        <button class="btn btn-warning" id="resetButton" data-ng-disabled="loginVm.forgotForm.$invalid" data-ng-click="loginVm.forgotForm.$valid && loginVm.forgotPassword($event)">
                            Reset My Password
                        </button>
                    </form>
                    <p></p>
                    <div data-ng-class="{'alert': true, 'alert-success': loginVm.forgotSuccess === true, 'alert-danger': loginVm.forgotSuccess === false}"class="alert alert-success ng-hide" data-ng-show="loginVm.forgotSent" data-ng-cloak>
                        <span data-ng-class="{'glyphicon': true, 'glyphicon-ok': loginVm.forgotSuccess === true, 'glyphicon-exclamation-sign': loginVm.forgotSuccess === false }"></span> {{loginVm.forgotMessage}}
                    </div>
                </div>
                <br>
                <div data-ng-class="{'text-center':true, 'alert alert-danger': loginVm.loginError === true}">
                    {{loginVm.message}}
                </div>
                <div class="ng-hide" data-ng-show="loginVm.showLoadingStars===true" data-ng-cloak>
